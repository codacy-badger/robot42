{
  "name": "robot-42",
  "version": "1.0.0",
  "description": "Robot42: The Toy Robot other Toy Robots look up to",
  "main": "run.js",
  "license": "Unlicense",
  "scripts": {
    "babel-node": "babel-node",
    "lint": "eslint src/**",
    "start": "npm run babel-node ./src/run.js",
    "start:watch": "nodemon --exec npm run babel-node ./src/run.js",
    "start:debug": "nodemon --inspect --exec npm run babel-node ./src/run.js",
    "test": "jest src",
    "test:watch": "jest src --watch",
    "coverage": "jest --collectCoverageFrom=src/**.js --coverage src",
    "coverage:watch": "jest --watch --collectCoverageFrom=src/**.js --coverage src"
  },
  "devDependencies": {
    "babel-cli": "^6.24.0",
    "babel-eslint": "^8.2.3",
    "babel-plugin-transform-object-rest-spread": "^6.26.0",
    "babel-preset-env": "^1.7.0",
    "eslint": "^4.19.1",
    "eslint-config-airbnb-base": "^12.1.0",
    "eslint-plugin-babel": "^5.1.0",
    "eslint-plugin-import": "^2.12.0",
    "eslint-plugin-jest": "^21.17.0",
    "eslint-plugin-sort-class-members": "^1.3.1",
    "jest": "^23.1.0"
  }
}
